{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb-typescript"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/jsx-filename-extension": [0],
    "import/extensions": [0],
    "import/no-extraneous-dependencies": [0]
  },
  "env": {
    "browser": true,
    "node": true
  }
}
